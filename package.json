    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "extract": "formatjs extract  'src/**/*.tsx' --out-file src/utils/messages/en.json --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
